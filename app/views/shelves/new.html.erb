<h1>Add a new shelf</h1>
<%= form_for [current_user, @shelf] do |f| %>
  <% if @shelf.errors.any? %>
    <ul>
      <% @shelf.errors.full_messages.each do |error| %>
        <li><%= error %></li>
      <% end %>
    </ul>
  <% end %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit "Save" %>
<% end %>
