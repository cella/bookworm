<h1>My shelves</h1>
<% if @shelves.any? %>
  <table>
    <thead>
    <tr>
      <th>Title</th>
    </tr>
    </thead>
    <tbody>
  <% @shelves.each do |shelf| %>
    <tr>
      <td><%= link_to shelf.title, user_shelf_path(shelf.user, shelf) %></td>
    </tr>
  <% end%>
    </tbody>
  </table>
<% end %>
